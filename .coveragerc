[run]
source = .
omit = 
    tests/*
    */tests/*
    */test_*.py
    venv/*
    .venv/*
    htmlcov/*
    logs/*
    .benchmarks/*
    setup.py
    */site-packages/*
    __pycache__/*
    .pytest_cache/*
    build/*
    dist/*

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    if self.debug
    if settings.DEBUG
    raise AssertionError
    raise NotImplementedError
    if 0:
    if __name__ == "__main__":
    pass
    except ImportError
    if typing.TYPE_CHECKING:

[html]
directory = htmlcov